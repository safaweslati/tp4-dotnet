@{
}
@model IEnumerable<Student> ; 

    <h1> Students</h1>
<table class="table">
    @if(Model.Count() == 0)
    {
          <a href="#" class="list-group-item list-group-item-action list-group-item-danger">No Students In This Course</a>
    }
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.first_name</td>
                    <td>@item.last_name</td>
                </tr>
        }
    </tbody>
</table>
<a asp-controller="Course" asp-action="index">All Courses</a>

